/* Chat */
.chat-box
    height: 100%
    margin: 0px
    padding-bottom: 0px
    padding-left: 5px
    padding-right: 5px
    overflow-y: auto
    list-style-type: none

.chat-box li span
    width: 100%
    display: block
    word-wrap: break-word
    
.input-chat
    outline: none

