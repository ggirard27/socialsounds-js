@media (max-width: 992px)
    .left-div
        left: 0
        right: 0
        bottom: 0
        position: absolute
        width: 100%
        padding-bottom: 5px
        padding-left: 15px
        padding-right: 15px

    .center-div
        width: 100%
        padding-right: 0px
        padding-left: 0px

    .right-div
        display: none
        left: 0
        right: 0
        bottom: 0
        position: absolute
        width: 100%
        background-color: #f2f2f2
        padding-bottom: 5px
        padding-left: 15px
        padding-right: 15px

    .owner-dashboard-section-title
        box-shadow: inset 0 -1px 0 0 #dad8de
        z-index: 5
        margin-bottom: 3px
        padding-top: 5px

    .playlist-section-title
        box-shadow: inset 0 -1px 0 0 #dad8de
        z-index: 5
        padding-bottom: 4px
        padding-top: 5px

    .playlist-section
        top: 37px
        left: 0
        right: 0
        bottom: 85px
        position: absolute
        padding-left: 15px
        padding-right: 15px

    .content-queue-list-group
        height: 100%
        overflow: auto

    .skip-section
        bottom: 0
        left: 0
        right: 0
        position: absolute
        padding-left: 15px
        padding-right: 15px

    .chat-section-title
        box-shadow: inset 0 -1px 0 0 #dad8de
        z-index: 5
        margin-top: 5px
        margin-bottom: 5px
        padding-bottom: 5px
        
    .chat-section
        top: 40px
        right: 0
        bottom: 0
        left: 0
        position: absolute
        width: 100%
        margin: 0px
        padding-bottom: 5px
        padding-left: 15px
        padding-right: 15px

    .chat-messages
        padding-bottom: 35px
        width: 100%
        height: 100%

    .chat-interface
        left: 0
        right: 0
        bottom: 0
        width: 100%
        position: absolute
        padding-bottom: 5px
        padding-left: 15px
        padding-right: 15px
/* end @media (max-width: 992px) */

@media (min-width: 992px)
    .page-title
        padding-right: 300px

    .left-div
        display: initial
        float: left
        height: 100%
        width: 300px
        padding-right: 5px

    .center-div
        height: 100%
        padding-right: 300px
        padding-left: 300px
    
    .right-div
        display: initial
        top: 50px
        right: 0
        bottom: 0
        position: absolute
        width: 300px
        background-color: #f2f2f2
        padding-left: 5px
        padding-right: 5px
        
    .owner-dashboard-section-title
        text-align: center
        box-shadow: inset 0 -1px 0 0 #dad8de
        z-index: 5
        margin-bottom: 3px
        padding-top: 5px

    .playlist-section-title
        text-align: center
        box-shadow: inset 0 -1px 0 0 #dad8de
        z-index: 5
        padding-bottom: 4px
        padding-top: 5px

    .playlist-section
        top: 130px
        bottom: 90px
        position: absolute
        width: inherit
        padding-right: 5px
    
    .content-queue-list-group
        height: 100%
        overflow: auto

    .skip-section
        bottom: 0
        width: inherit
        position: absolute

    .chat-section-title
        text-align: center
        box-shadow: inset 0 -1px 0 0 #dad8de
        z-index: 5
        margin-top: 14px
        margin-bottom: 10px
        padding-bottom: 5px
        
    .chat-section
        top: 45px
        right: 0
        bottom: 0
        position: absolute
        width: 100%
        margin: 0px

    .chat-messages
        top: 0
        bottom: 42px
